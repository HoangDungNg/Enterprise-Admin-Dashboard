    "@syncfusion/ej2-react-kanban": "^23.2.4",
    "@syncfusion/ej2-react-popups": "^23.2.4",
    "@syncfusion/ej2-react-richtexteditor": "^23.2.7",
    "@syncfusion/ej2-react-schedule": "^23.2.7",
    "autoprefixer": "^10.4.16",
    "postcss": "^8.4.32",
